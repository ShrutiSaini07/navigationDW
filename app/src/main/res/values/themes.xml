<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NavProject" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/grad_1</item>
        <item name="colorPrimaryVariant">@color/grad_2</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FFFFFF</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="21">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/regular</item>
    </style>


    <style name="floating" parent="Widget.Design.FloatingActionButton">
        <item name = "android:foreground">
            @drawable/button
        </item>
    </style>

    <style name="floating1" parent="Widget.Design.FloatingActionButton">
        <item name = "android:foreground">
            @drawable/sos
        </item>
    </style>


    <style name="floating2" parent="Widget.Design.FloatingActionButton">
        <item name = "android:foreground">
            @drawable/trackme
        </item>
    </style>

    <style name="floating3" parent="Widget.Design.FloatingActionButton">
        <item name = "android:foreground">
            @drawable/image
        </item>
    </style>

    <style name="floating4" parent="Widget.Design.FloatingActionButton">
        <item name = "android:foreground">
            @drawable/video
        </item>
    </style>

    <style name="floating5" parent="Widget.Design.FloatingActionButton">
        <item name = "android:foreground">
            @drawable/live
        </item>
    </style>



</resources>